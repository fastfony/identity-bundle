{
    "name": "fastfony/identity-bundle",
    "description": "Symfony bundle focused on identification and the management of users, roles and groups",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "identity", "user", "role", "group", "authentication"],
    "authors": [
        {
            "name": "Fastfony",
            "email": "info@fastfony.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/doctrine-bridge": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/rate-limiter": "^6.4|^7.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "doctrine/orm": "^2.14|^3.0",
        "doctrine/doctrine-bundle": "^2.7",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/mailer": "^7.3",
        "symfony/notifier": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "twig/extra-bundle": "^3.21",
        "twig/cssinliner-extra": "^3.21",
        "twig/inky-extra": "^3.21",
        "symfony/translation": "^7.3",
        "symfony/uid": "^7.3",
        "symfony/form": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Fastfony\\IdentityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fastfony\\IdentityBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
